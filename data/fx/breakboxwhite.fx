VertexShader = [[attribute vec3 pos;attribute vec4 col;attribute vec3 nor;attribute vec2 uv;uniform mat4 proj;uniform mat4 view;uniform mat4 world;uniform vec4 scale=vec4(1,1,0,0);varying HIGHP vec2 vtx_uv;varying vec2 coords[14];void main(){    gl_Position = vec4((pos+vec3(-0.5,-0.5,0))*2.0,1.0);    vtx_uv = pos.xy;}]]FragmentShader = [[uniform sampler2D color_tex; uniform vec4 scolor;uniform sampler2D srcBuf;uniform sampler2D srcBuf_depth;uniform vec4 param;uniform vec4 time;uniform vec4 demotime;varying HIGHP vec2 vtx_uv;varying vec2 coords[14];void main(){	float aa = max(0.0, demotime.x - 41.57);	gl_FragColor = vec4(2.0,2.0,2.0,aa);	}]]GeometryShader = ""DepthTest = falseBlend = trueBlendSrc = "SRC_ALPAH"BlendDst = "ONE"Priority=10