VertexShader = [[attribute vec3 pos;attribute vec4 col;attribute vec3 nor;attribute vec2 uv;uniform sampler2D color_tex;uniform mat4 proj;uniform mat4 view;uniform mat4 world;uniform vec4 p1;varying HIGHP vec2 vtx_uv;varying LOWP vec4    vtx_col;varying HIGHP vec4 vtx_pos;void main(void){	vtx_uv = uv;	vtx_col = col;	vtx_pos = world * vec4(pos,1.0);	gl_Position = proj * view * vtx_pos;}]]FragmentShader = [[uniform sampler2D color_tex; uniform vec4 color;uniform vec4 time;uniform vec4 p1;uniform vec4 fft;varying HIGHP vec2 vtx_uv;varying LOWP vec4  vtx_col;varying HIGHP vec4 vtx_pos;float PI = 3.1415926;void main(void){	float d = min(2.0,(vtx_pos.y)*10.0);/*	float cr = fft.z*20.0;	float cb = fft.y*20.0;	float cg = fft.x*10.0;	float tt = floor(vtx_pos.y*10.0);	tt = mod(tt,3.0);	if (tt == 0.0)		cb = cg = 0.0;	else if (tt == 1.0)		cr = cb = 0.0;	else		cr = cg = 0.0;*///	gl_FragColor = vec4(d+cr,d+cg,d+cb,1.0) *color * texture2D(color_tex, vtx_uv);	gl_FragColor = vec4(d,d,d,1.0) *color * texture2D(color_tex, vtx_uv);}]]GeometryShader = ""DepthTest = trueBlend = trueBlendSrc = "SRC_ALPAH"BlendDst = "ONE_MINUS_SRC_ALPHA"Priority=0